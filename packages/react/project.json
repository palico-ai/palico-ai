{
  "name": "react",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/react/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "main": "packages/react/src/index.ts",
        "outputPath": "dist/packages/react",
        "tsConfig": "packages/react/tsconfig.lib.json",
        "compiler": "tsc",
        "generateExportsField": true,
        "project": "packages/react/package.json",
        "format": ["esm", "cjs"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs react {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "publish-local": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "cwd": "dist/packages/react",
        "commands": [
          "npm unpublish --force --registry http://localhost:4873",
          "npm publish --registry http://localhost:4873"
        ],
        "parallel": false
      }
    }
  }
}
